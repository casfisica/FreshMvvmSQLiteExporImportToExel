<?xml version="1.0" encoding="utf-8" ?>
<freshmvvm:FreshBaseContentPage  xmlns:freshmvvm="clr-namespace:FreshMvvm;assembly=FreshMvvm" 
                                 xmlns="http://xamarin.com/schemas/2014/forms"
                                 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                                 x:Class="FreshMvvmSQLiteExporImportToExel.Pages.MainPage"
                                 Title="Pagina Principal">
    <ContentPage.Content>
        <StackLayout HorizontalOptions="Center"
                     VerticalOptions="Center">
            <Label Text="Add User" HorizontalOptions="Center"
                     VerticalOptions="Center"/>
            <Button Text="Presione aqui"
                    Command="{Binding GoToListPage}"
                    VerticalOptions="CenterAndExpand"/>
        </StackLayout>
    </ContentPage.Content>
</freshmvvm:FreshBaseContentPage>